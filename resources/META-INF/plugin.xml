<idea-plugin>
  <id>monokai-pro</id>
  <name>Monokai Pro</name>
  <version>0.1</version>
  <vendor>Jonathan Esposito</vendor>

  <description>
    <![CDATA[
      Monokai Pro is a color scheme and customized user interface theme for code editors. It has been designed with beautiful functionality in mind. Carefully selected shades of colors are the foundation of an uncompromising, non-distractive user interface.
    ]]>
  </description>

  <idea-version since-build="191.0"/>
  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="1324eea6-b737-4305-8a73-14af69073eae-1" path="/Monokai Pro.theme.json"/>
    <themeProvider id="1324eea6-b737-4305-8a73-14af69073eae-2" path="/Monokai Pro Machine.theme.json"/>
    <themeProvider id="1324eea6-b737-4305-8a73-14af69073eae-3" path="/Monokai Pro Octagon.theme.json"/>
    <themeProvider id="1324eea6-b737-4305-8a73-14af69073eae-4" path="/Monokai Pro Ristretto.theme.json"/>
    <themeProvider id="1324eea6-b737-4305-8a73-14af69073eae-5" path="/Monokai Pro Spectrum.theme.json"/>
  </extensions>

</idea-plugin>
